<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffffff" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginTop="5dp"
        android:background="#cccccc" >

    <TextView
        android:id="@+id/complete_textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="140dp"
        android:text="completed"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/deliver_textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/complete_textView2"
        android:layout_alignBottom="@+id/complete_textView2"
        android:layout_alignParentRight="true"
        android:layout_marginRight="16dp"
        android:text="Delivered" 
        android:textColor="#000000" />

    <TextView
        android:id="@+id/orderlist_textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView1"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="orderlist"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/timetextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/orderlist_textView1"
        android:layout_alignParentBottom="true"
        android:text="Time" />

    <RelativeLayout
        android:id="@+id/RelativeLayout"
        android:layout_width="0.5dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="240dp"
        android:layout_marginTop="6dp"
        android:background="#000000" >

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeLayout1"
        android:layout_width="0.5dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="115dp"
        android:layout_marginTop="6dp"
        android:background="#000000" >

    </RelativeLayout>

</RelativeLayout>
</RelativeLayout>
